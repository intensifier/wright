  "license": "WTFPL",
  "main": "lib/index.js",
  "bin": {
    "wright": "./bin/wright"
  },
  "preferGlobal": true,
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "try:rollup": "npm install && cd examples/rollup && npm install && node wright",
    "try:mithril": "npm install && cd examples/mithril && npm install && node rollup",
    "try:simple": "npm install && cd examples/simple && node ../../bin/wright -r \"window.reload()\" index.html",
    "test": "tape test/**"
  },
  "repository": "porsager/wright",
  "dependencies": {
    "chokidar": "3.2.1",
    "dotenv": "8.1.0",
    "ey": "0.7.0",
    "finalhandler": "1.1.2",
    "http-proxy": "1.18.0",
    "inquirer": "6.5.1",
    "minimist": "1.2.0",
    "pws": "4.1.2",
    "serve-static": "1.14.1",
    "source-map": "0.7.3",
    "ubre": "0.0.20",
    "ws": "7.1.2"
  },
  "devDependencies": {
    "bss": "1.6.3",
    "mithril": "1.1.7",
    "rollup": "1.23.1",
    "rollup-plugin-buble": "0.19.8",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "tape": "4.11.0"
  }
